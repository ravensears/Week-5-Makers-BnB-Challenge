
<ul>
  <% @space.each do |space| %>
    <li>
        <%= space.name %>
        <%= space.description %>
        <%= space.price %>
        <br>
        Available From:    
        <%= space.start_date %>
        to
        <%= space.end_date %>
        <form method="get" action="/spaces/<%= space.id %>">
          <input type="submit" value="View Space">
        </form>
    </li>
  <% end %>
</ul>
